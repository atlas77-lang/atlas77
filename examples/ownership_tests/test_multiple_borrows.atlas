import "std/io";

extern str_len(s: &const string) -> uint64;
extern str_concat(s1: &const string, s2: &const string) -> string;

public struct MyString {
public:
    s: string;
    len: uint64;
    uppercase: string;

public:
    MyString(str: string) {
        this.s = str;
        this.len = str_len(&str);
        this.uppercase = str_concat(&str, &str);
    }
}

fun main() {
    let s = new MyString("hello");
    println(s.s);
}
