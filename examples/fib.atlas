import "std/io";

fun main() {
    println(fib(5));
}

fun fib(n: int64) -> int64 {
    if n <= 1 {
        return n;
    } else {
        return fib(n - 1) + fib(n - 2);
    }
}

struct TestAddingCopyConstructor {
public:
    value: int64;
    TestAddingCopyConstructor(v: int64) {
        this.value = v;
    }
    //Let's see if the _copy constructor is added correctly by the compiler
}
